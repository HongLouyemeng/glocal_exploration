<launch>
  <param name="/use_sim_time" value="true"/>
  <arg name="rate" default="2"/>
  <arg name="rviz_file" default="global_planner" />

<!-- sliding_window, global_map, local_area, local_planner, global_planner-->


  <!-- Play Rosbag and RVIZ (visualization) -->
  <node pkg="rosbag" type="play" name="player" output="screen" args="-r $(arg rate) /home/unreal/glocal_data/video_bags/active_3d/drift4.bag"/>	

  <node type="rviz" name="rviz" pkg="rviz" args="-d /home/unreal/glocal_data/video_bags/glocal/$(arg rviz_file).rviz"/>
</launch>
